#include<bits/stdc++.h>
using namespace std;
const int N =105;
int v[N],w[N];

bool cmp(int a,int b)
{
	return a<b;
}

int main()
{
	int t;cin>>t;int n;
	while(t--)
	{
		cin>>n;
		for(int i=1;i<=n;i++)	cin>>v[i];
		for(int i=1;i<=n;i++)	cin>>w[i];
		
		sort(v+1,v+1+n,cmp);
		sort(w+1,w+1+n,cmp);
		int k=1;
		for(int i=1;i<=n;i++)
		{
			while(w[i]>v[k]&&k<=n)
			{
				k++;
			}
			v[k]-=w[i];
		}
		
		if(k<=n)	printf("YES");
		else printf("NO");
	}
	return 0;
}
