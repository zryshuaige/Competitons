////
#include<bits/stdc++.h>
using namespace std;
const int N = 2*1e5+5;
int a[N],s[N];

bool cmp(int a,int b)
{
	return a<b;
}

int main()
{
	int t,n;cin>>t;
	while(t--)
	{
		cin>>n;
		int ans,k;
		for(int i=1;i<=n;i++)
			scanf("%d",a[i]);
		cin>>k;
		sort(a+1,a+1+n,cmp);
		for(int i=1;i<=n;i++)
			s[i]=s[i-1]+a[i];
		
	}
	return 0;
}
